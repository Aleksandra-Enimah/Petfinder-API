n    display: grid;\r\n    place-items: center;\r\n    position: relative;\r\n    overflow-y: scroll;\r\n    padding-top: 5px;\r\n    \r\n}\r\n  .close-btn {\r\n    position: absolute;\r\n    top: 0.5rem;\r\n    right: 1rem;\r\n    font-size: 2.5rem;\r\n    background: transparent;\r\n    border-color: transparent;\r\n    color: #690303;\r\n    cursor: pointer;\r\n}\r\n.article {\r\n    color: #f3ecec;\r\n    display: flex;\r\n    font-size: 20px;\r\n    background-color: #171b1b;\r\n    margin-bottom: 10px;\r\n    margin-top: 15px;\r\n    width: 100%;\r\n    gap: 10px;\r\n    \r\n}\r\n#img-box {\r\n   text-align: center;\r\n  \r\n}\r\n#pet {\r\n    margin: auto;\r\n    display: block;\r\n    border: 10px solid #171b1b;\r\n    height: auto;\r\n    max-height: 150px;\r\n    top: auto;\r\n    left: auto;\r\n}\r\n.item1 {\r\n      width: 30%;\r\n}\r\n  .item2 {\r\n      position: relative;\r\n      text-align:start;\r\n      \r\n}\r\n  .description {\r\n      padding-left: 35px;\r\n}\r\n  li {\r\n      list-style-type: none;\r\n    \r\n}\r\n  .list {\r\n    position: absolute;\r\n    bottom: 0;\r\n    left: 0;\r\n    text-align: left;\r\n  }\r\n#alarm-message {\r\n    margin-top: 15px;\r\n    background-color: #c20e0e;\r\n    color: #f3ecec;\r\n    border: 5px solid white;\r\n    font-family: 'Oswald', sans-serif;\r\n    position: absolute;\r\n    left:0;\r\n    right:0;\r\n    margin-left:auto;\r\n    margin-right:auto;\r\n    width: 50%;\r\n    font-size: 17px;\r\n    padding: 5px;\r\n}\r\n.alarm {\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n@media screen and (max-width: 600px) {\r\n    .found-container {\r\n      width: 100vw;\r\n      height: 100vh; \r\n      font-size: 25px;\r\n    }\r\n    .close-btn {\r\n        top: 0.3rem;\r\n        right: 0rem;\r\n        font-size: 2rem;  \r\n    }\r\n    .article {\r\n        font-size: 15px;\r\n        margin-bottom: 10px;\r\n        margin-top: 15px;\r\n        width: 100%;\r\n        gap: 5px;\r\n        \r\n    }\r\n\